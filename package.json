{
  "name": "compaz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start_pwa": "npm run build && npx serve build",
    "build": "react-scripts build",
    "test": "npm run lint && react-scripts test --watchAll=false",
    "only_test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings=0 && prettier --check 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "lint_fix": "eslint . --fix --ext .js,.ts,.tsx && prettier --write 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "update_deps": "npm outdated || true && npm update && npm outdated || true && npm install && npm test && npm run build && npm run loki:ci && git add . && git commit -m \"update deps\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && loki update",
    "loki:test": "npm run build-storybook && loki test",
    "loki:ci": "npm run build-storybook && loki --requireReference --verboseRenderer",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:ci"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.12.0",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "dijkstrajs": "^1.0.2",
    "downshift": "^6.1.7",
    "firebase": "^9.8.3",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "lodash.debounce": "^4.0.8",
    "loki": "^0.30.3",
    "mockdate": "^3.0.5",
    "re-resizable": "^6.9.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-firebase-hooks": "^5.0.3",
    "react-i18next": "^11.17.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.2",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4",
    "wicg-inert": "^3.1.2",
    "workbox-background-sync": "^6.5.3",
    "workbox-broadcast-update": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-google-analytics": "^6.5.3",
    "workbox-navigation-preload": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-streams": "^6.5.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/addon-viewport": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/node-logger": "^6.5.8",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/google.maps": "^3.49.2",
    "@types/jest": "^28.1.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cypress": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-testing-library": "^5.5.1",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "webpack": "^5.73.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "storybook-static"
  ],
  "loki": {
    "reactUri": "file:./storybook-static",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1680,
        "height": 1050,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false,
        "storiesFilter": "normal"
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7",
        "mobile": true,
        "storiesFilter": "mobile"
      }
    }
  }
}
