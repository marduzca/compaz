{
  "name": "compaz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start_pwa": "npm run build && npx serve build",
    "build": "react-scripts build",
    "test": "npm run lint && react-scripts test --watchAll=false",
    "only_test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings=0 && prettier --check 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "lint_fix": "eslint . --fix --ext .js,.ts,.tsx && prettier --write 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "upgrade_deps": "npm outdated || true && npm update && npm outdated || true && npm install && npm test && npm run build && npm run loki:ci && git add . && git commit -m \"update deps\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "loki:update": "npm run build-storybook && loki update",
    "loki:test": "npm run build-storybook && loki test",
    "loki:ci": "npm run build-storybook && loki --requireReference --verboseRenderer"
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/node": "^16.10.9",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "dijkstrajs": "^1.0.2",
    "downshift": "^6.1.7",
    "firebase": "^8.10.0",
    "i18next": "^21.3.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "loki": "^0.28.1",
    "mockdate": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-i18next": "^11.12.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.2",
    "workbox-background-sync": "^6.3.0",
    "workbox-broadcast-update": "^6.3.0",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-google-analytics": "^6.3.0",
    "workbox-navigation-preload": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-range-requests": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-streams": "^6.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "storybook-static"
  ],
  "loki": {
    "reactUri": "file:./storybook-static",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1680,
        "height": 1050,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false,
        "storiesFilter": "normal"
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7",
        "mobile": true,
        "storiesFilter": "mobile"
      }
    }
  }
}
