{
  "name": "compaz",
  "version": "0.23.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:pwa": "npm run build && npx serve build",
    "pre-commit": "npm run build && npm test && npm run loki:test",
    "build": "react-scripts build",
    "test": "npm run lint && react-scripts test --watchAll=false",
    "only-test": "react-scripts test --watchAll=false",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings=0 && prettier --check 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx && prettier --write 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "update-app-version": "sed -E -i '' \"s/const APP_VERSION =.*/const APP_VERSION = '$npm_package_version';/g\" src/app/App.tsx && git add . && git commit -m \"Release new version\"",
    "release:major": "npm version major && npm run update-app-version",
    "release:minor": "npm version minor && git push && echo 'Wait 1 min for first commit to start running in pipeline...' && sleep 60 && npm run update-app-version && git push",
    "update-deps": "npm outdated || true && npm update && npm outdated || true && npm install && npm test && npm run build && npm run loki:ci && git add . && git commit -m \"update deps\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && loki update",
    "loki:test": "npm run build-storybook && loki test",
    "loki:ci": "npm run build-storybook && loki --requireReference --verboseRenderer",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:ci",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.13.1",
    "dijkstrajs": "^1.0.2",
    "downshift": "^7.0.1",
    "firebase": "^9.12.1",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^1.4.5",
    "lodash.debounce": "^4.0.8",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-i18next": "^11.18.6",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.4",
    "wicg-inert": "^3.1.2",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-viewport": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/google.maps": "^3.50.4",
    "@types/jest": "^29.2.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-testing-library": "^5.9.1",
    "history": "^5.3.0",
    "loki": "^0.30.3",
    "mockdate": "^3.0.5",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "webpack": "^5.74.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "storybook-static"
  ],
  "loki": {
    "reactUri": "file:./storybook-static",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1680,
        "height": 1050,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false,
        "storiesFilter": "normal"
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7",
        "mobile": true,
        "storiesFilter": "mobile"
      }
    }
  }
}
