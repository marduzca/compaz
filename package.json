{
  "name": "compaz",
  "version": "0.23.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "start:pwa": "npm run build && npx serve build",
    "build": "GENERATE_SOURCEMAP=false tsc && vite build",
    "serve": "vite preview",
    "pre-commit": "npm run build && npm test",
    "test": "npm run lint && vitest run",
    "only-test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 && prettier --check 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx && prettier --write 'src/**/*.{js,ts,tsx,css,scss,json,html}'",
    "release:major": "npm version major && npm run update-app-version",
    "release:minor": "npm version minor && git push && echo 'Wait 1 min for first commit to start running in pipeline...' && sleep 60 && npm run update-app-version && git push",
    "update-deps": "npm outdated || true && npm update && npm outdated || true && npm install && npm run pre-commit && git add . && git commit -m \"update deps\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "loki:update": "concurrently --prefix none --kill-others 'npm run storybook' 'loki update'",
    "loki:test": "concurrently --prefix none --kill-others 'npm run storybook' 'loki test'",
    "loki:ci": "npm run build-storybook && loki --requireReference --verboseRenderer --reactUri file:./storybook-static",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:ci"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.18.1",
    "@vitejs/plugin-react": "^4.0.0",
    "dijkstrajs": "^1.0.2",
    "downshift": "^7.6.0",
    "firebase": "^9.19.1",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.10.0",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.15.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.1.0",
    "vitest": "^0.31.0",
    "web-vitals": "^3.3.1",
    "wicg-inert": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@storybook/addon-actions": "^7.0.4",
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-interactions": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/react": "^7.0.4",
    "@storybook/react-vite": "^7.0.4",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/google.maps": "^3.52.5",
    "@types/node": "^20.1.1",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.1.0",
    "cypress": "^12.9.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.2",
    "history": "^5.3.0",
    "jsdom": "^22.0.0",
    "loki": "^0.31.1",
    "mockdate": "^3.0.5",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.8.7",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "storybook-static"
  ],
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1680,
        "height": 1050,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false,
        "storiesFilter": "normal"
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7",
        "mobile": true,
        "storiesFilter": "mobile"
      }
    }
  }
}
